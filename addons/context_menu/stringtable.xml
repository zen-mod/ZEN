<?xml version="1.0" encoding="utf-8"?>
<Project name="ZEN">
    <Package name="Context_Menu">
        <Key ID="STR_ZEN_Context_Menu_DisplayName">
            <English>Context Menu</English>
        </Key>
        <Key ID="STR_ZEN_Context_Menu_Enabled">
            <English>Enable Context Menu</English>
            <French>Activer le menu contextuel</French>
            <Russian>Вкл. контекстное меню</Russian>
            <German>Kontextmenu aktivieren</German>
            <Polish>Włącz menu kontekstowe</Polish>
            <Japanese>コンテキスト有効化</Japanese>
            <Korean>맞춤형 메뉴 활성화</Korean>
        </Key>
        <Key ID="STR_ZEN_Context_Menu_Enabled_Description">
            <English>Enables opening of context menu using the keybind or right clicking.</English>
            <French>Permet d'ouvrir le menu contextuel à l'aide d'une touche ou d'un clic droit.</French>
            <German>Ermöglicht das Öffnen des Kontextmenus mit einer Tastenkombination oder Rechtslick.</German>
            <Polish>Pozwala otwierać menu kontekstowe używając skrótu klawiaturowego lub prawego przycisku myszy.</Polish>
            <Russian>Включает открытие контекстного меню по правой клавише мыши или сочетанию клавиш</Russian>
            <Japanese>割り当てられたキーか右クリックによりコンテキスト メニューを使えるようにします。</Japanese>
            <Korean>키 바인드 또는 마우스 우클릭을 사용해 맞춤형 메뉴를 열 수 있습니다.</Korean>
        </Key>
        <Key ID="STR_ZEN_Context_Menu_KeybindOnly">
            <English>Keybind Only</English>
            <French>Touche uniquement</French>
            <Russian>Только сочетание клавиш</Russian>
            <German>Nur Tastenkombination</German>
            <Polish>Tylko skrót klawiaturowy</Polish>
            <Japanese>割り当てキーからのみ</Japanese>
            <Korean>키 바인드만 사용 가능</Korean>
        </Key>
        <Key ID="STR_ZEN_Context_Menu_KeybindAndMouse">
            <English>Keybind and Mouse</English>
            <French>Touche et souris</French>
            <Russian>Мышь и сочетание клавиш</Russian>
            <German>Tastenkombination und Rechtsklick</German>
            <Polish>Skrót klawiaturowy i mysz</Polish>
            <Japanese>割り当てキーとマウス</Japanese>
            <Korean>마우스 우클릭만 사용 가능</Korean>
        </Key>
        <Key ID="STR_ZEN_Context_Menu_OverrideWaypoints">
            <English>Override Waypoint Placement</English>
        </Key>
        <Key ID="STR_ZEN_Context_Menu_OverrideWaypoints_Description">
            <English>Allows for the context menu to be opened by right clicking when waypoint placement would normally occur.\nRequires holding the SHIFT key in order to place waypoints.</English>
        </Key>
        <Key ID="STR_ZEN_Context_Menu_OpenContextMenu">
            <English>Open Context Menu</English>
            <French>Ouvrir le menu contextuel</French>
            <Russian>Открыть контекстное меню</Russian>
            <German>Kontextmenu öffnen</German>
            <Polish>Otwórz menu kontekstowe</Polish>
            <Japanese>コンテキスト メニューを開く</Japanese>
            <Korean>맞춤형 메뉴 열기</Korean>
        </Key>
    </Package>
</Project>
